      "displayName": "Top 20 Mills by Net Sales",
      "queryLines": [
        "SELECT\n",
        "  mill_name,\n",
        "  SUM(net_sales) net_sales\n",
        "FROM\n",
        "  genie_poc.gold.fact_sales\n",
        "GROUP BY\n",
        "  1\n",
        "ORDER BY\n",
        "  mill_name DESC\n",
        "LIMIT 20;"
